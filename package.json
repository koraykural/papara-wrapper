{
  "name": "papara-wrapper",
  "version": "1.0.5",
  "description": "Papara API wrapper for node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write src/**/*.ts"
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koraykural/papara-wrapper.git"
  },
  "keywords": [
    "papara",
    "papara api"
  ],
  "author": "Koray Kural",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koraykural/papara-wrapper/issues"
  },
  "homepage": "https://github.com/koraykural/papara-wrapper#readme",
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^10.0.0",
    "axios-logger": "^2.8.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.9.0",
    "uuid": "^11.1.0"
  }
}
